<%= render "shared/profile_header" %>
<%= render "no_photos" if @photos.empty? %>
<%= render "shared/flash" %>
<div class="container">
  <% @photos.each do |photo| %>
    <div class="col-md-4 index-photo">
      <%= link_to image_tag(photo.picture.url), user_photo_path(photo.user, photo) %>   
    </div>
  <% end %>
</div>
    
